<div class="wrapper col3">
  <div id="container">
    <div id="content">
     </div>
        <div>
        <table border='1'>
            <tr>

                <td>Username</td>
                <td>Fee</td>
                <td>Date</td>
             <tr>
                 
        @foreach (var std in Model)
        {
            if(@std.Fee1!=null)
            {
             <tr>
                 <td>@std.Student_Username</td>
                 <td>@std.Fee1</td>
                 <td>@std.Date</td>
              </tr>
            }
            else
            {
                <tr>
                 <td>@std.Student_Username</td>
                 <td>Not-Payed</td>
                 <td>@std.Date</td>
              </tr>
            }
        }    
        </table>
        </div>
